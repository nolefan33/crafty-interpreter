class BaseClass {
    fun sayHello() {
        print "Hello, ";
    }
}

class WorldClass < BaseClass {
    fun sayWorld() {
        var times = 1;
        for (var i = 0; i < times; i = i + 1) {
            print "World";
        }
    }
}

/*
 * Entry point to program.
 * /* nested comment bb */
 */
fun main() {
    var speaker = WorldClass()
    speaker.sayHello();
    speaker.sayWorld();
}
